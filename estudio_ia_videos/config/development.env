# =================================================
# ESTÚDIO IA VÍDEOS - CONFIGURAÇÃO DE DESENVOLVIMENTO
# =================================================

# Database - Supabase (valores padrão para desenvolvimento)
DATABASE_URL="postgresql://postgres:password@localhost:5432/estudio_ia_videos"

# Supabase Configuration
NEXT_PUBLIC_SUPABASE_URL=https://demo-project.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=demo_anon_key_placeholder
SUPABASE_SERVICE_ROLE_KEY=demo_service_role_key_placeholder

# NextAuth.js Configuration
NEXTAUTH_SECRET=demo_nextauth_secret_key_minimum_32_chars_long_placeholder
NEXTAUTH_URL=http://localhost:3000

# OpenAI API (placeholder)
OPENAI_API_KEY=sk-demo_openai_key_placeholder

# Desenvolvimento
NODE_ENV=development
NEXT_PUBLIC_APP_VERSION=1.0.0
NEXT_PUBLIC_ENVIRONMENT=development

# Feature Flags
NEXT_PUBLIC_ENABLE_COLLABORATION=true
NEXT_PUBLIC_ENABLE_AI_TEMPLATES=true
NEXT_PUBLIC_ENABLE_AR_PREVIEW=true
NEXT_PUBLIC_ENABLE_QUANTUM_STUDIO=false
